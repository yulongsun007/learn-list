<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="sayHelloServiceRmi" class="org.springframework.remoting.rmi.RmiProxyFactoryBean" >
        <property name="serviceUrl" value="rmi://127.0.0.1:9999/helloWorldServiceName" />
        <property name="serviceInterface"    value="win.yulongsun.demo.spring.rmi.server.IHelloWorld"/>
    </bean>
</beans>