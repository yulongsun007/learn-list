server:
  port: 8761
eureka:
  server:
    # registerWithEureka 是否将当前应用注册到EurekaServer,默认为true
    # 由于当前应用就是EurekaServer,所以设置为false
    registerWithEureka: false
    # 表示是否从EurekaServer获取注册信息,默认为true
    fetchRegistry: false
    serviceUrl:
      # 设置与EurekaServer交互的地址。
      # 查询和注册服务都需要依赖这个地址。
      defaultZone: http://localhost:8761/eureka