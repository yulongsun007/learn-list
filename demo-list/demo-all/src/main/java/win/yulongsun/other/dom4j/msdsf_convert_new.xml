      <identitytypeSet name="1" value="88888" describe="h"/>
      <identitytypeSet name="2" value="ZR04" describe="j"/>  
      <identitytypeSet name="3" value="ZR06" describe="士兵证"/>  
      <identitytypeSet name="4" value="ZR09" describe="ssssss"/>  
